{
  "name": "concrete-isomorphic-react",
  "version": "0.0.1",
  "description": "Isomorphic react boilerplate with hapi",
  "main": "server.js",
  "homepage": "https://github.com/mcrider/typewriter",
  "repository": {
    "type": "git",
    "url": "git://github.com/mcrider/typewriter.git"
  },
  "author": "Matt Crider",
  "license": "MIT",
  "scripts": {
    "client": "gulp",
    "server": "nodemon server.js",
    "dev": "npm run client & npm run server",
    "build": "gulp build --type production && cd ../server && rm -rf public/* && cp -r ../client/build/* public/",
    "production": "npm run build && npm run server",
    "deploy": "npm run build && node deploy.js"
  },
  "devDependencies": {
    "gulp": "~3.8.10",
    "gulp-browserify": "~0.5.0",
    "gulp-sass": "*",
    "gulp-watch": "~0.6.5",
    "gulp-connect": "~2.0.5",
    "reactify": "~0.17.1",
    "gulp-concat": "~2.1.7",
    "gulp-imagemin": "~0.1.4",
    "gulp-clean": "~0.3.0",
    "jquery": "~2.1.0",
    "envify": "~1.2.1",
    "gulp-livereload": "^2.1.1",
    "livereactload": "^0.2.1"
  },
  "dependencies": {
    "boom": "^2.6.1",
    "colors": "^1.0.3",
    "gulp-if": "^1.2.5",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.1",
    "hapi": "^8.2.0",
    "joi": "^5.1.0",
    "localforage": "^1.2.2",
    "mongoose": "^3.8.23",
    "node-jsx": "^0.12.4",
    "node-neat": "^1.3.0",
    "node-notifier": "^4.1.0",
    "nodemon": "^1.3.7",
    "prismic.io": "^1.1.4",
    "promise": "^6.0.1",
    "react": "^0.13.1",
